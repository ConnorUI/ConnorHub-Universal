-- Movement Page

local Page = UI.New({
	Title = "Movement"
})

Page.Slider({
	Text = "Speed",
	Callback = function(value)
		Plr.Character.Humanoid.WalkSpeed = value
	end,
	Min = 18,
	Max = 100,
	Def = 16
})

Page.Slider({
	Text = "Jump Power",
	Callback = function(value)
		Plr.Character.Humanoid.JumpPower = value
	end,
	Min = 60,
	Max = 200,
	Def = 50
})

Page.Button({
	Text = "Reset Speed",
	Callback = function()
		Plr.Character.Humanoid.WalkSpeed = 16
	end
})

Page.Button({
	Text = "Reset Jump Power",
	Callback = function()
		Plr.Character.Humanoid.JumpPower = 50
	end
})

Page.Button({
	Text = "Infinite Jump (Rejoin To Disable)",
	Callback = function()
		InfJump = true

		game:GetService("UserInputService").JumpRequest:Connect(function()
			if InfJump then
				game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping")
			end
		end)
	end
})

Page.Button({
	Text = "Lag Switch (X Toggle)",
	Callback = function()
		loadstring(game:HttpGet("https://paste.gg/p/anonymous/d3492bd620bd4c46b54680ac80b0fa0d/files/161ddd9c992c4ceba3c298458d45188a/raw", true))()
	end
})

Page.Button({
	Text = "NoClip (Rejoin To Disable)",
	Callback = function()
		local noclip = true char = game.Players.LocalPlayer.Character while true do if noclip == true then for _,v in pairs(char:children()) do pcall(function() if v.className == "Part" then v.CanCollide = false elseif v.ClassName == "Model" then v.Head.CanCollide = false end end) end end game:service("RunService").Stepped:wait() end
	end
})

-- Options Page

local Page = UI.New({
	Title = "Options"
})

Page.Toggle({
	Text = "Day/Night",
	Callback = function(Day)
		if Day then
			game.Lighting.ClockTime = 9
		else
			game.Lighting.ClockTime = 23
		end
	end,
	Enabled = true,
})

Page.Button({
	Text = "Remove Light Effects",
	Callback = function()
		game.Lighting.Blur.Enabled = false
		game.Lighting.Bloom.Enabled = false
		game.Lighting.ColorCorrection.Enabled = false
		game.Lighting.DepthOfField.Enabled = false
		game.Lighting.SunRays.Enabled = false
		game.Lighting.GlobalShadows = false
		game.Lighting.Brightness = 1
	end
})

Page.Button({
	Text = "FPS Booster",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ConnorUI/FPS-Booster/main/Script",true))();
	end
})

Page.Button({
	Text = "Hide Name (Random)",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ConnorUI/UserSpoof/main/Script",true))();
	end
})

Page.Button({
	Text = "High Graphics (Might Make You Lag!)",
	Callback = function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/ConnorUI/Graphics-Roblox/main/Script",true))();
	end
})

